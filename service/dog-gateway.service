 
[Unit]
Description=Dog OSS Gateway
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
User=dog
Group=dog
WorkingDirectory=/opt/dog-gateway
ExecStart=/usr/bin/java -Xmx128m -Dorg.osgi.service.http.port=8080 -DconfigFolder=/etc/dog/config -jar org.eclipse.osgi_3.16.200.v20210226-1447.jar -clean -console 65300
Restart=always
RestartSec=5
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=dog-gateway


[Install]
WantedBy=default.target
